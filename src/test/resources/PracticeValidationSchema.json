
{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "required": [
    "id",
    "name",
    "gender",
    "phone"
  ],
  "properties": {
    "id": {
      "type": "integer",
      "default": 0,
      "title": "The id Schema",
      "examples": [
        119
      ]
    },
    "name": {
      "type": "string",
      "default": "",
      "title": "The name Schema",
      "examples": [
        "Mehmet"
      ]
    },
    "gender": {
      "type": "string",
      "default": "",
      "title": "The gender Schema",
      "examples": [
        "Male"
      ]
    },
    "phone": {
      "type": "integer",
      "default": 0,
      "title": "The phone Schema",
      "examples": [
        1234567890
      ]
    }
  },
  "examples": [{
    "id": 119,
    "name": "Mehmet",
    "gender": "Male",
    "phone": 1234567890
  }]
}